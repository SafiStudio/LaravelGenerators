{
	"name": "safistudio/generators",
	"description": "Laravel artisan command generating MVC structure of app components",
	"keywords": ["generators", "laravel"],
	"license": "MIT",
	"type": "library",
	"require": {
	  "php": ">=5.5.9"
	},
	"autoload": {
		"classmap": [
			"src"
		],
		"psr-4": {
		  "SafiStudio\\": ""
		}
	},
	"scripts": {
		"post-install-cmd": [
			"\\SafiStudio\\Installer::postPackageInstall"
		],
		"post-update-cmd": [
		  	"\\SafiStudio\\Installer::postUpdate"
		]
	}
}
